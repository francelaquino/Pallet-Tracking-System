@{
    Layout = "~/Areas/en/Views/Shared/_LayoutPage1.cshtml";
}

<div class="row" ng-controller="trackingController" ng-init="getPalletCount();">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                <h2>Tracking <small> Pallet Count </small></h2>

                <div class="clearfix"></div>
            </div>
            <div class="x_content">
                
                <div class="datatable" style="display:none">
                    <table id="datatable" class="table table-striped table-bordered">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>RFID</th>
                                <th>Count</th>




                            </tr>
                        </thead>


                        <tbody>
                            <tr ng-repeat="pallet in pallets">
                                <td>{{ pallet.EMPLOYEE }}</td>
                                <td>{{ pallet.RFID }}</td>
                                <td>{{ pallet.CNT }}</td>







                            </tr>

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>


